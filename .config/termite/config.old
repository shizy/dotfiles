[options]
scroll_on_output = false
scroll_on_keystroke = true
audible_bell = false
mouse_autohide = false
allow_bold = true
dynamic_title = true
urgent_on_bell = true
clickable_url = true
font = M+ 1m 9
scrollback_lines = 10000
search_wrap = true
cursor_blink = system
cursor_shape = block

# set size hints for the window
#size_hints = false

# emit escape sequences for other keys modified by Control
#modify_other_keys = false

[colors]
#cursor = #dcdccc
foreground = #A0A0A0
foreground_bold = #66D9EF
background = rgba(0, 0, 0, 0.85)

# if unset, will reverse foreground and background
#highlight = #FD971F

#color0 = #303030
#color1 = #F92672
#color2 = #A6E22E
#color3 = #FD971F
#color4 = #AE81FF
#color5 = #EF5939
#color6 = #66D9EF
#color7 = #CFCFCF
color0 = #181818
color1 = #282828
color2 = #383838
color3 = #585858
color4 = #B8B8B8
color5 = #D8D8D8
color6 = #E8E8E8
color7 = #F8F8F8

color8 = #686868
color9 = #F92672
color10 = #A6E22E
color11 = #FD971F
color12 = #AE81FF
color13 = #EF5939
color14 = #66D9EF
color15 = #FFFFFF

[hints]
#font = Monospace 12
#foreground = #dcdccc
#background = #3f3f3f
#active_foreground = #e68080
#active_background = #3f3f3f
#padding = 2
#border = #3f3f3f
#border_width = 0.5
#roundness = 2.0

# vim: ft=dosini cms=#%s
