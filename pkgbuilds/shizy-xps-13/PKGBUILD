depends=(
    'alsa-utils'
    'dialog'
    'intel-ucode'
    'iw'
    'wpa_supplicant'
    'wpa_actiond'
)
md5sums=('SKIP')
install=hooks.install

source=(
    i915.conf
)

package() {
    install -Dm 644 $srcdir/i915.conf $pkgdir/etc/modprobe.d/i915.conf
}
